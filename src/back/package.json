{
  "name": "roadkill-reporter-api",
  "version": "1.0.2",
  "private": true,
  "description": "API for Utah Roadkill Reporter mobile app.",
  "license": "MIT",
  "author": "Scott Davis <stdavis@utah.gov>",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "lint": "eslint .",
    "serve": "env-cmd nodemon server.js",
    "setup:emulator": "firebase emulators:start --import=../../data/emulator-data --export-on-exit --only firestore,storage",
    "start": "firebase emulators:exec --import=../../data/emulator-data --only firestore,storage --ui \"npm run serve\"",
    "test": "env-cmd -f .env.test vitest",
    "test:ci": "firebase emulators:exec --only firestore,storage \"npm test\""
  },
  "jest": {
    "transform": {}
  },
  "dependencies": {
    "@google-cloud/firestore": "^6.2.0",
    "@google-cloud/logging-winston": "^5.1.5",
    "@google-cloud/storage": "^6.5.0",
    "@sendgrid/mail": "^7.7.0",
    "common": "file:../common",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "express-winston": "^4.2.0",
    "fast-crc32c": "^2.0.0",
    "got": "^12.5.0",
    "helmet": "^6.0.0",
    "humanize-duration": "^3.27.3",
    "jwt-decode": "^3.1.2",
    "knex": "^2.3.0",
    "mime": "^3.0.0",
    "multer": "^1.4.4",
    "pg": "^8.8.0",
    "semver": "^7.3.7",
    "sharp": "^0.31.0",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "firebase-tools": "^11.9.0",
    "form-urlencoded": "^6.1.0",
    "msw": "^0.47.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.1.1",
    "prettier-plugin-packagejson": "^2.2.18",
    "supertest": "^6.2.4",
    "typescript": "^4.8.3",
    "vitest": "^0.23.4"
  }
}
