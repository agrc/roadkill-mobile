{
  "name": "wildlife-vehicle-collision-reporter-api",
  "version": "1.0.0",
  "description": "API for Utah Wildlife-Vehicle Collision Reporter mobile app.",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "emulate": "lsof -ti :3001 | xargs kill -9 && gcloud beta emulators firestore start --host-port=localhost:3001",
    "lint": "eslint .",
    "run-ci-tests": "env-cmd -f .env.test node --experimental-vm-modules node_modules/.bin/jest",
    "run-tests": "env-cmd -f .env.test node --experimental-vm-modules node_modules/.bin/jest --watch",
    "serve": "env-cmd nodemon server.js",
    "start": "concurrently npm:emulate npm:serve",
    "test": "concurrently npm:emulate npm:run-tests",
    "test:ci": "concurrently --kill-others npm:emulate npm:run-ci-tests"
  },
  "author": "Scott Davis <stdavis@utah.gov>",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/firestore": "^4.14.1",
    "@google-cloud/logging-winston": "^4.1.0",
    "@sendgrid/mail": "^7.4.5",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-winston": "^4.1.0",
    "got": "^11.8.2",
    "helmet": "^4.5.0",
    "jwt-decode": "^3.1.2",
    "knex": "^0.95.7",
    "pg": "^8.6.0",
    "winston": "^3.3.3",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "concurrently": "^6.0.2",
    "env-cmd": "^10.1.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "form-urlencoded": "^6.0.3",
    "jest": "^26.6.3",
    "msw": "^0.28.2",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "prettier-plugin-organize-imports": "^2.2.0",
    "supertest": "^6.1.3",
    "typescript": "^4.3.5"
  },
  "private": true,
  "jest": {
    "transform": {}
  }
}
