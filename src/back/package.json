{
  "name": "wildlife-vehicle-collision-reporter-api",
  "version": "1.0.0",
  "description": "API for Utah Wildlife-Vehicle Collision Reporter mobile app.",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "run-tests:ci": "env-cmd -f .env.test node --experimental-vm-modules node_modules/.bin/jest",
    "serve": "env-cmd nodemon server.js",
    "start": "firebase emulators:exec --import=../../data/emulator-data --export-on-exit --only firestore,storage \"npm run serve\"",
    "test": "env-cmd -f .env.test node --experimental-vm-modules node_modules/.bin/jest --watch",
    "test:ci": "firebase emulators:exec --import=../../data/emulator-data --export-on-exit --only firestore,storage \"npm run run-tests:ci\""
  },
  "author": "Scott Davis <stdavis@utah.gov>",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/firestore": "^4.14.1",
    "@google-cloud/logging-winston": "^4.1.0",
    "@google-cloud/storage": "^5.14.2",
    "@sendgrid/mail": "^7.4.5",
    "common": "file:../common",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-winston": "^4.1.0",
    "fast-crc32c": "^2.0.0",
    "got": "^11.8.2",
    "helmet": "^4.5.0",
    "jwt-decode": "^3.1.2",
    "knex": "^0.95.7",
    "knex-postgis": "^0.14.1",
    "mime": "^2.5.2",
    "multer": "^1.4.3",
    "pg": "^8.6.0",
    "sharp": "^0.29.2",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "firebase-tools": "^9.16.0",
    "form-urlencoded": "^6.0.3",
    "jest": "^27.0.6",
    "msw": "^0.28.2",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "prettier-plugin-organize-imports": "^2.2.0",
    "supertest": "^6.1.6",
    "typescript": "^4.3.5"
  },
  "private": true,
  "jest": {
    "transform": {}
  }
}
