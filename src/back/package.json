{
  "name": "roadkill-reporter-api",
  "version": "1.0.3",
  "private": true,
  "description": "API for Utah Roadkill Reporter mobile app.",
  "license": "MIT",
  "author": "Scott Davis <stdavis@utah.gov>",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "lint": "eslint .",
    "serve": "env-cmd nodemon server.js",
    "setup:emulator": "firebase emulators:start --import=../../data/emulator-data --export-on-exit --only firestore,storage",
    "start": "firebase emulators:exec --import=../../data/emulator-data --only firestore,storage --ui \"npm run serve\"",
    "test": "env-cmd -f .env.test vitest",
    "test:ci": "firebase emulators:exec --only firestore,storage \"npm test\""
  },
  "dependencies": {
    "@google-cloud/firestore": "^7.6.0",
    "@google-cloud/logging-winston": "^6.0.0",
    "@google-cloud/storage": "^7.10.1",
    "@sendgrid/mail": "^8.1.3",
    "apple-signin-auth": "^1.7.6",
    "common": "file:../common",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-winston": "^4.2.0",
    "fast-crc32c": "^2.0.0",
    "helmet": "^7.1.0",
    "humanize-duration": "^3.32.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "jwt-decode": "^4.0.0",
    "knex": "^3.1.0",
    "ky": "^1.2.4",
    "mime": "^4.0.1",
    "multer": "^1.4.4",
    "pg": "^8.11.5",
    "semver": "^7.6.0",
    "sharp": "^0.33.3",
    "uuid": "^9.0.1",
    "winston": "^3.13.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.1.0",
    "firebase-tools": "^13.7.3",
    "form-urlencoded": "^6.1.4",
    "msw": "^2.2.14",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-packagejson": "^2.5.0",
    "supertest": "^6.3.4",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0"
  }
}
