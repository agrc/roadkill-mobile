{
  "name": "wildlife-vehicle-collision-reporter-api",
  "version": "1.0.0",
  "description": "API for Utah Wildlife-Vehicle Collision Reporter mobile app.",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "emulate": "lsof -ti :3001 | xargs kill -9 && gcloud beta emulators firestore start --host-port=localhost:3001",
    "lint": "eslint .",
    "serve": "env-cmd nodemon server.js",
    "start": "concurrently npm:emulate npm:serve",
    "test": "env-cmd -f .env.test node --experimental-vm-modules node_modules/.bin/jest --watch"
  },
  "author": "Scott Davis <stdavis@utah.gov>",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/firestore": "^4.10.1",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "got": "^11.8.2",
    "helmet": "^4.5.0",
    "jwt-decode": "^3.1.2"
  },
  "devDependencies": {
    "concurrently": "^6.0.2",
    "env-cmd": "^10.1.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "form-urlencoded": "^6.0.3",
    "jest": "^26.6.3",
    "msw": "^0.28.2",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3"
  },
  "private": true,
  "jest": {
    "transform": {}
  }
}
