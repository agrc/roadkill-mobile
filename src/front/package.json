{
  "name": "roadkill-reporter",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "android": "expo run:android",
    "eas-build-pre-install": "bash ./scripts/eas-build-pre-install.sh",
    "format": "prettier --write .",
    "ios": "expo run:ios",
    "lint": "eslint .",
    "prestart": "sb-rn-get-stories",
    "start": "expo start --dev-client",
    "storybook-generate": "sb-rn-get-stories --config-path=./.storybook",
    "storybook-watch": "sb-rn-watcher",
    "test": "env-cmd -f .env.test jest --watch --coverage=false",
    "test:debug": "env-cmd -f .env.test jest -o --watch --coverage=false",
    "test:final": "npm run lint && env-cmd -f .env.test jest && npm run validate-translations",
    "update-constants": "env-cmd -f .env node scripts/updateConstants.mjs",
    "update-snapshots": "jest -u --coverage=false",
    "validate-translations": "node scripts/validateTranslations.mjs"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "./jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|native-base|@sentry/.*|autolinker|@ui-kitten/.*|p-timeout|i18n-js|cheap-ruler)"
    ]
  },
