    "android": "expo start --android",
    "eject": "expo eject",
    "ios": "expo start --ios",
    "lint": "eslint .",
    "postinstall": "patch-package",
    "start": "expo start",
    "test": "env-cmd -f .env.test jest --watch --coverage=false --changedSince=origin/main",
    "test:debug": "env-cmd -f .env.test jest -o --watch --coverage=false",
    "test:final": "env-cmd -f .env.test jest",
    "update-snapshots": "jest -u --coverage=false",
    "web": "expo start --web"
  },
